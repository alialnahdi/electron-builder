{
  "name": "my-electron-app",
  "version": "1.0.2",
  "description": "My Electron App with auto-update",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder "
  },
  "author": "alialnahdi",
  "license": "ISC",
  "dependencies": {
    "electron-log": "^5.4.0",
    "electron-updater": "^6.6.2"
  },
  "devDependencies": {
    "electron": "^36.1.0",
    "electron-builder": "^26.0.12"
  },
  "build": {
    "appId": "com.alialnahdi.autoupdate",
    "publish": [
      {
        "provider": "github",
        "owner": "alialnahdi",
        "repo": "electron-builder"
      }
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alialnahdi/electron-builder.git"
  }
}
